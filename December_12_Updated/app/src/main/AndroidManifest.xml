<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dharamraz">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/dharamraj_app_icon5"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.main.SignIn" />
        <activity android:name="com.main.AboutUs" />
        <activity android:name="com.main.CountriesList" />
        <activity android:name="com.main.HomeActivity" />
        <activity android:name="com.main.EmailInsert" />
        <activity android:name="com.GridPack.ActivityGridExa" />
        <activity android:name="com.main.SearchActivity" />
        <activity
            android:name="com.main.Countries"

            android:label="@string/title_country_name" />
        <activity
            android:name="example.dharamraz.SearchActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name="com.GridPack.GamesActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="com.GridPack.SearchNewActivity"
            android:theme="@style/AppTheme.Dark" />

        <activity
            android:name="com.GridPack.UrlActivity"
            android:theme="@style/AppTheme.Dark" />

    </application>

</manifest>